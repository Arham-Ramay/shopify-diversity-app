{
  "version": 3,
  "builds": [
    {
      "src": "build/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/build/client/(.*)",
      "dest": "/build/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  }
}